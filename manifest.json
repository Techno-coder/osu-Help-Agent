{
  "manifest_version": 2,
  "name": "osu! Help Agent",
  "version": "0.1.1",
  "description": "Adds tools for managing topics in the osu! help forum.",
  "icons": {
	"48": "icons/icon.svg",
	"96": "icons/icon.svg"
  },
  "content_scripts": [
	{
	  "matches": [
		"https://osu.ppy.sh/*"
	  ],
	  "js": [
		"external/browser-polyfill.min.js",
		"src/tags.js",
		"src/index.js"
	  ],
	  "css": [
		"src/index.css"
	  ]
	}
  ],
  "permissions": [
	"storage"
  ]
}

